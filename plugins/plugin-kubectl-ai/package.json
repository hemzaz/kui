{
  "name": "@kui-shell/plugin-kubectl-ai",
  "version": "13.1.0",
  "description": "AI-powered Kubernetes assistant plugin for Kui",
  "license": "Apache-2.0",
  "keywords": [
    "kubernetes",
    "kubectl",
    "ai",
    "assistant",
    "cli",
    "ui",
    "kui",
    "plugin"
  ],
  "author": "Kui Contributors",
  "homepage": "https://github.com/IBM/kui#readme",
  "bugs": {
    "url": "https://github.com/IBM/kui/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IBM/kui.git"
  },
  "main": "dist/index.js",
  "typedocMain": "src/index.ts",
  "module": "mdist/index.js",
  "types": "mdist/index.d.ts",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
