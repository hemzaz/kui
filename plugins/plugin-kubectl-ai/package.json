    "test:verbose": "jest --verbose",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/node-cache": "^4.2.5",
    "@types/markdown-it": "^14.1.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "openai": "^4.52.0",
    "node-cache": "^5.1.2",
    "dotenv": "^16.4.5",
    "markdown-it": "^14.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
